OBJECTS = divi.o add.o sub.o mult.o
staticout : staticlib.a
	cd ..;gcc -I./lib -L./lib main.c $^ -o $@
divi.o : divi.c
	cd lib; gcc -c -fPIC $<
add.o : add.c
	gcc -c $<
sub.o : sub.c
	gcc -c $<
mult.o : mult.c
	gcc -c $<
staticlib.a : $(OBJECTS)
	ar rcs $@ $^
clean :
	rm -f staticout;cd lib;rm -f staticlib.a  $(OBJECTS)
